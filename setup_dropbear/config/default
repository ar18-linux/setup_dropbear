# Should be the same port as your openssh port
ar18_port=22

# Modules that need loading for error-free usage on your system
declare -ag ar18_modules
ar18_modules=(
  iwlwifi
  e1000e
)

# Public keys as found in ~/.ssh/*.pub
declare -ag ar18_public_keys
ar18_public_keys=(
  "ssh-rsa  nulysses@AR-XUBUNTU18"
)
